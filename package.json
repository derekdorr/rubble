{
  "name": "skald",
  "version": "2.0.0",
  "description": "Shakeable, tested javascript utility functions in a pure functional programming style",
  "keywords": [
    "client",
    "functional",
    "modules",
    "util",
    "server"
  ],
  "repository": "https://github.com/derekdorr/skald",
  "browser": "browser/index.js",
  "main": "lib/index.js",
  "modules": "es/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "browser",
    "lib",
    "es",
    "src",
    "browser",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "docs": "gulp docs",
    "test": "cross-env BABEL_ENV=lib nyc tape tests/*-test.js",
    "lint": "eslint --fix src/ tests/",
    "build-browser": "rollem",
    "build-lib": "cross-env BABEL_ENV=lib babel src --out-dir lib",
    "build-es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build-lib && npm run build-es && npm run build-browser",
    "precommit": "npm run lint && npm run test"
  },
  "nyc": {
    "exclude": [
      "tests/"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "author": "Derek Dorr",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^8.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-imports": "^1.4.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.13.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "glob": "^7.1.2",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-jsdoc-to-markdown": "^1.2.2",
    "gulp-rename": "^1.2.2",
    "husky": "^1.1.3",
    "nyc": "^14.1.1",
    "rollem": "^1.12.0",
    "rollup": "^0.60.1",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "sinon": "^7.4.1",
    "tape": "^4.11.0"
  },
  "dependencies": {
    "permanent": "^1.0.0"
  }
}
